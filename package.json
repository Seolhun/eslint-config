{
  "name": "@seolhun/eslint-config",
  "description": "Eslint configuration with prettier",
  "version": "1.4.1",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "prettier": "prettier --write './**/*.{ts,tsx,js,jsx}'",
    "changeset:add": "changeset add",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish"
  },
  "author": "Seolhun",
  "files": [
    "index.js",
    "prettier.config.js"
  ],
  "bugs": {
    "url": "https://github.com/seolhun/eslint-config/issues"
  },
  "homepage": "https://github.com/seolhun/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seolhun/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "typescript",
    "javascript",
    "prettier",
    "react"
  ],
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=8",
    "prettier": ">=3",
    "typescript": ">=5"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-eslint-plugin": "^6.3.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^3.9.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.4.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@types/react": "^18.3.12",
    "react": "^18.3.1"
  }
}
