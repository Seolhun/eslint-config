  },
  "homepage": "https://github.com/seolhun/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seolhun/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "typescript",
    "javascript",
    "prettier",
    "react"
  ],
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=9",
    "prettier": ">=3",
    "typescript": ">=5"
  },
  "dependencies": {
