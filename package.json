{
  "name": "@seolhun/eslint-config",
  "description": "Eslint configuration with prettier for TypeScript, React, and Node.js",
  "version": "2.1.1",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./base": "./base.js",
    "./react": "./react.js",
    "./prettier": "./prettier.config.js"
  },
  "scripts": {
    "test": "pnpm lint && pnpm lint:base && pnpm typecheck",
    "lint": "eslint test/",
    "lint:base": "eslint --config test/eslint.config.base.js test/test-typescript.ts test/test-javascript.js",
    "lint:fix": "eslint test/ --fix",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write './**/*.{ts,tsx,js,jsx}'",
    "changeset:add": "changeset add",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish"
  },
  "author": "Seolhun",
  "files": [
    "index.js",
    "base.js",
    "react.js",
    "prettier.config.js",
    "types.d.ts"
  ],
  "bugs": {
    "url": "https://github.com/seolhun/eslint-config/issues"
  },
  "homepage": "https://github.com/seolhun/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seolhun/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "typescript",
    "javascript",
    "prettier",
    "react",
    "nestjs",
    "nextjs"
  ],
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=9",
    "eslint-plugin-jsx-a11y": ">=6",
    "eslint-plugin-react": ">=7",
    "eslint-plugin-react-hooks": ">=5",
    "prettier": ">=3",
    "typescript": ">=5"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/js": "^9.39.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-eslint-plugin": "^6.5.0",
    "eslint-plugin-perfectionist": "^5.6.0",
    "eslint-plugin-prettier": "^5.5.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^25.3.0",
    "@types/react": "^19.1.8",
    "eslint": "^9.39.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.8.1",
    "react": "^19.1.0"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
